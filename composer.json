{
  "name": "thoronir42/paf",
  "description": "Portfolio application with quote functionality",
  "type": "project",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Thoronir42",
      "email": "Thoronir42@Gmail.com"
    }
  ],
  "scripts": {
    "phpcs": "phpcs app/ test/ extensions/",
    "phpcbf": "phpcbf app/ test/ extensions/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.3.0",

    "nette/application": "dev-master#c99c04ed665e69ca547df6c490d2ccc47d47855f",
    "nette/bootstrap": "^3.0",
    "nette/caching": "^3.0",
    "nette/database": "^3.0",
    "nette/di": "^3.0-RC5",
    "nette/finder": "^2.5|^3.0",
    "nette/forms": "^3.0",
    "nette/http": "^3.0",
    "nette/mail": "^3.0",
    "nette/robot-loader": "^3.0",
    "nette/safe-stream": "^2.4",
    "nette/security": "^3.0",
    "nette/utils": "^3.0",
    "latte/latte": "^2.5",
    "tracy/tracy": "^2.6",
    "tharos/leanmapper": "^3.4",

    "sestep/navigation": "dev-master",
    "sestep/modular-lean-mapper": "dev-master",
    "contributte/translation": "^0.4.1",
    "contributte/console": "^0.7.1",
    "contributte/monolog": "^0.3.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Thoronir42/application"
    }
  ],
  "autoload": {
    "psr-4": {
      "PAF\\": "app/",
      "SeStep\\Commentable\\": "extensions/SeStep/Commentable/src/",
      "SeStep\\FileAttachable\\": "extensions/SeStep/FileAttachable/src/",
      "SeStep\\GeneralSettings\\": "extensions/SeStep/GeneralSettings/src/",
      "SeStep\\GeneralSettingsInMemory\\": "extensions/SeStep/GeneralSettingsInMemory/src/",
      "SeStep\\EntityIds\\": "extensions/SeStep/EntityIds/src/",
      "SeStep\\LeanSettings\\": "extensions/SeStep/LeanSettings/src/",
      "SeStep\\LeanFixtures\\": "extensions/SeStep/LeanFixtures/src/",
      "SeStep\\NavigationMenuComponent\\": "extensions/SeStep/NavigationMenuComponent/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\PAF\\": "test/",
      "Test\\SeStep\\GeneralSettings\\": "extensions/SeStep/GeneralSettings/test/",
      "Test\\SeStep\\GeneralSettingsInMemory\\": "extensions/SeStep/GeneralSettingsInMemory/test/",
      "Test\\SeStep\\LeanSettings\\": "extensions/SeStep/LeanSettings/test/",
      "SeStep\\EntityIds\\": "extensions/SeStep/EntityIds/test/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.2",
    "squizlabs/php_codesniffer": "^3.4"
  }
}
